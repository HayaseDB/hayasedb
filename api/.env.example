# Server port (default: 3000)
API_PORT=3000

# Environment: development | test | staging | production (default: development)
API_ENV=development

# CORS origin (optional, leave empty to disable)
# API_CORS_ORIGIN=http://localhost:3000

# Enable Swagger documentation (default: true)
API_SWAGGER_ENABLED=true

# Swagger documentation path (default: docs)
API_SWAGGER_PATH=docs
